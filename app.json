{
  "expo": {
    "name": "Mintwave",
    "slug": "firstmate-mintwave",
    "owner": "firstmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "scheme": "privy-expo",
    "splash": {
      "image": "./assets/synthwave/synthwave_image-3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.firstmate.echo",
      "supportsTablet": true
    },
    "android": {
      "package": "com.firstmate.echo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "15673973-37bf-464d-bbc8-5a6a9cd34b5b"
      }
    }
  }
}
